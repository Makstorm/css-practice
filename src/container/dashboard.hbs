<html>

  <head>
    <title>
      Dashboard
    </title>
    <meta charset='utf-8' />
    <link href='css/normalize.css' rel='stylesheet' />
    <link rel='stylesheet' href='css/dashboard.css' />
    <meta
      name='viewport'
      content='viewport-fit=cover, width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no'
    />

  </head>

  <body>
    <div class='page page--dashboard'>

      <aside
        class='page__aside aside vertical vertial--big'
      >
        <div class='aside__logo text'>
          Sidebar
        </div>
        <nav class='vertical vertical--small'>
          <a
            href=''
            class='nav-button nav-button--active click'
          >
            <span
              class='icon nav-button__icon'
              style="background-image: url('/svg/nav/home.svg');"
            ></span>
            <span class='text text--light'>Home</span>
          </a>

          <a href='' class='nav-button click'>
            <span
              class='icon nav-button__icon'
              style="background-image: url('/svg/nav/orders.svg');"
            ></span>
            <span class='text text--light'>Orders</span>
          </a>

          <a href='' class='nav-button click'>
            <span
              class='icon nav-button__icon'
              style="background-image: url('/svg/nav/products.svg');"
            ></span>
            <span class='text text--light'>Products</span>
          </a>

        </nav>
      </aside>

      <header class='header'>
        <div
          class='layout header__content text text--small'
        >
          <div class='header__search relative'>
            <input
              type='text'
              placeholder='Search'
              class='field field--header'
            />
            <span
              class='icon header__icon-search click'
              style="background-image: url('/svg/header/search.svg');"
            ></span>
            <span
              class='icon icon--header-settings icon--button icon--color-blue click'
              style="background-image: url('/svg/header/settings.svg');"
            ></span>
          </div>

          <div class='header__icons flex flex--big'>
            <span
              class='icon header-right__icon icon--button icon--color-yellow click'
              style="background-image: url('/svg/header/antena.svg');"
            ></span>
            <span
              class='icon header-right__icon icon--button icon--color-blue-light click'
              style="background-image: url('/svg/header/symbols.svg');"
            ></span>
            <span
              class='icon header-right__icon icon--button icon--color-yellow click'
              style="background-image: url('/svg/header/bell.svg');"
            ></span>
          </div>
        </div>
      </header>

      <main class='main'>
        <div class='layout'>
          <section class='box analytic__block text'>
            <div
              class='analytic analytic--double analytic--purple'
            >
              <span
                class='icon analytic__button icon--button icon--color-purple click'
                style="background-image: url('/svg/analytic/money.svg');"
              ></span>
              <div
                class='relative vertical vertical--small'
              >
                <div
                  class='analytic__title text text--light'
                >
                  $500.00
                </div>
                <div class='text text--small text--lighter'>
                  Total Earning
                </div>
                <span
                  class='icon analytic__arrow'
                  style="background-image: url('/svg/analytic/blue-arrow.svg');"
                ></span>

              </div>
            </div>

            <div
              class='analytic analytic--double analytic--blue'
            >
              <span
                class='icon analytic__button icon--button icon--color-blue click'
                style="background-image: url('/svg/analytic/order.svg');"
              ></span>
              <div
                class='relative vertical vertical--small'
              >
                <div
                  class='analytic__title text text--light'
                >
                  $961
                </div>
                <div class='text text--small text--lighter'>
                  Total Order
                </div>
                <span
                  class='icon analytic__arrow'
                  style="background-image: url('/svg/analytic/blue-arrow.svg');"
                ></span>

              </div>
            </div>

            <div
              class='analytic analytic--blue analytic--row'
            >
              <span
                class='icon analytic__button icon--button icon--color-blue click'
                style="background-image: url('/svg/analytic/money.svg');"
              ></span>
              <div class='vertical vertical--small'><div
                  class='text text--bigger text--light'
                >
                  $203k
                </div>
                <div class='text text--small text--lighter'>
                  Total Income
                </div>
              </div>

            </div>

            <div
              class='analytic analytic--transparent analytic--row'
            >
              <span
                class='icon analytic__button icon--button icon--color-yellow click'
                style="background-image: url('/svg/analytic/shop.svg');"
              ></span>
              <div class='vertical vertical--small'><div
                  class='text text--bigger'
                >
                  $203k
                </div>
                <div class='text text--small'>
                  Total Income
                </div>
              </div>

            </div>
          </section>

        </div>

        <div class='layout'>
          <div class='main-container'>
            <section class='box purchase__block'>
              <div class='text text--heading'>Purchase list</div>
              <div class='divider'></div>
              <div class='row row--four'>
                <div class='vertical vertical--small'>
                  <div class='text'>ID437661</div>
                  <div class='text'>May 11, 2023, 14:21</div>
                </div>
                <div class='vertical vertical--small'>
                  <div class='text'>AirOn ProCam</div>
                  <div class='text'>7/8 USB Type-C</div>
                </div>
                <div class='vertical vertical--small'>
                  <div class='text'>132.32 EUR</div>
                  <div class='text text--red'>Not paid</div>
                </div>
                <div class='vertical vertical--small'>
                  <div class='text'>Customer</div>
                  <div class='text'>robin@com.de</div>
                </div>

              </div>
              <div class='divider'></div>
              <div class='row row--four'>
                <div class='vertical vertical--small'>
                  <div class='text'>ID437661</div>
                  <div class='text'>May 11, 2023, 14:21</div>
                </div>
                <div class='vertical vertical--small'>
                  <div class='text'>PHILIPS Essential</div>
                  <div class='text'>HD9200/90</div>
                </div>
                <div class='vertical vertical--small'>
                  <div class='text'>543 USD</div>
                  <div class='text text--green'>Pay by
                    Stripe</div>
                </div>
                <div class='vertical vertical--small'>
                  <div class='text'>Customer</div>
                  <div class='text'>dima@com.ua</div>
                </div>

              </div>
            </section>
            <section class='box payment__block'>
              <div class='text text--heading'>Payment</div>
              <div class='divider'></div>
              <div class='vertical'>
                <div class='form__checkbox'>
                  <div class='vertical vertical--small'>
                    <div class='checkbox flex'>
                      <input
                        type='radio'
                        id='Credit'
                        name='Radio'
                        value='Credit card'
                      />
                      <label
                        class='text'
                        for='Credit'
                      >Credit card</label>
                    </div>

                    <div class='checkbox flex'>
                      <input
                        type='radio'
                        id='Debit'
                        name='Radio'
                        value='Debit card'
                      />
                      <label class='text' for='Debit'>Debit
                        card</label>
                    </div>

                    <div class='checkbox flex'>
                      <input
                        type='radio'
                        id='PayPal'
                        name='Radio'
                        value='PayPal'
                      />
                      <label
                        class='text'
                        for='PayPal'
                      >PayPal</label>
                    </div>
                  </div>
                </div>
                <div class='row row--two-two'>
                  <div class='vertical vertical--x-small'>
                    <div class='text'>
                      Name on card
                    </div>
                    <div class='relative click'>
                      <div
                        class='field field--select relative'
                      >
                        <span class='text'></span>
                      </div>
                      <input class='field__select' />

                    </div>
                  </div>

                  <div class='vertical vertical--x-small'>
                    <div class='text'>
                      Credit card number
                    </div>
                    <div class='relative click'>
                      <div
                        class='field field--select relative'
                      >
                        <span class='text'></span>
                      </div>
                      <input class='field__select' />

                    </div>
                  </div>

                  <div class='vertical vertical--x-small'>
                    <div class='text'>
                      Expiration
                    </div>
                    <div class='relative click'>
                      <div
                        class='field field--select relative'
                      >
                        <span class='text'></span>
                      </div>
                      <input class='field__select' />

                    </div>
                  </div>

                  <div class='vertical vertical--x-small'>
                    <div class='text'>
                      CVV
                    </div>
                    <div class='relative click'>
                      <div
                        class='field field--select relative'
                      >
                        <span class='text'></span>
                      </div>
                      <input class='field__select' />

                    </div>
                  </div>

                </div>

              </div>
              <div class='divider'></div>

              <div class='row row--two'>
                <div class='vertical vertical--x-small'>
                  <div class='text'>
                    Country
                  </div>
                  <div class='relative click'>
                    <div
                      class='field field--select relative'
                    >
                      <span class='text'>Choose...</span>
                    </div>
                    <select class='field__select'>
                      <option value='dog'>Dog</option>
                      <option value='cat'>Cat</option>
                      <option value='bird'>Bird</option>
                    </select>

                    <span
                      class='icon down-arrow_icon'
                      style="background-image: url('/svg/form/down-arrow.svg');"
                    ></span>
                  </div>
                </div>

                <div class='vertical vertical--x-small'>
                  <div class='text'>
                    State
                  </div>
                  <div class='relative click'>
                    <div
                      class='field field--select relative'
                    >
                      <span class='text'>Choose...</span>
                    </div>
                    <select class='field__select'>
                      <option value='dog'>Dog</option>
                      <option value='cat'>Cat</option>
                      <option value='bird'>Bird</option>
                    </select>

                    <span
                      class='icon down-arrow_icon'
                      style="background-image: url('/svg/form/down-arrow.svg');"
                    ></span>
                  </div>

                </div>

              </div>

              <div class='divider'></div>

              <div
                class='button button--primary text click'
              >
                Continue to checkout
              </div>
            </section>
          </div>
        </div>
      </main>

    </div>
  </body>

</html>